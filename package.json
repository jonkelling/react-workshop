{
  "name": "react-workshop",
  "version": "1.0.0",
  "description": "Describe react-workshop here",
  "private": true,
  "scripts": {
    "build": "nwb build-react-app",
    "clean": "nwb clean-app",
    "start": "npx babel-node --harmony server.js",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {
    "csv": "^2.0.0",
    "linq": "^3.0.9",
    "moment": "^2.20.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-preset-react-hmre": "^1.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^4.15.0",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.1.4",
    "nwb": "^0.21.2"
  },
  "author": "",
  "license": "MIT",
  "repository": "",
  "babel": {
    "presets": [
      "react",
      "stage-1"
    ],
    "env": {
      "development": {
        "presets": [
          "env",
          "react-hmre"
        ]
      },
      "production": {
        "presets": [
          [
            "env",
            {
              "es2015": {
                "modules": false
              },
              "targets": {
                "ie": 9,
                "uglify": true
              }
            }
          ]
        ],
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-remove-prop-types"
        ]
      },
      "test": {
        "presets": [
          "env"
        ]
      }
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|svg|md)$": "<rootDir>/src/configs/empty-module.js"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "Container.js"
    ]
  }
}
